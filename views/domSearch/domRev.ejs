<%- include('../header') %>
<script language="JavaScript" src="../scripts/domeClass.js?v=<?=$v?>"></script>
<link rel="stylesheet" href="/css/international.css">
<main class=" international">

    <form name="frmForm" method="post" id="frmForm" onsubmit="return false">
		<input type="hidden" name = "tmpData" value ="" id = "tmpData"> 
		<input type="hidden" name = "tmpData2" value ="" id = "tmpData2"> 
		<input type="hidden" name="titleData" value="">
		<div id="searchFrame">
			<div class="search_action_bar">
				<div class="search_action_left">
					<div class="search-options">
						<div class="trip-type">
							<button class="trip active"  ID="ticketType2" onClick="return typeChange('2')">왕복</button>
							<button class="trip"  ID="ticketType1" onClick="return typeChange('1')">편도</button>
						</div>
					</div>
				</div>
            </div>

			<!-- < %- include('priceInfo.inc') % > -->

			<div class="search-bar">	
				<!--여행지역 선택-->
					<div class="search-item city-select" data-type="from">
						<span class="label">출발도시</span>
						<strong>
							<input type="text" class="city-input" name="depName" id="depName" onClick="startOpen()" readonly value="김포" placeholder="">
						</strong>
						<div class="area-layer" id="start_area" style="display:none;left:0px;top:81px;">
							<div class="area-layer_b">
								<ul ID="depArea">
									
								</ul>
							</div>
						</div>
					</div>

					<div class="swap">⇄</div>

					<div class="search-item city-select" data-type="to">
						<span class="label">도착도시</span>
						<strong>
							<input type="text" class="city-input" name="arrName" id="arrName" onClick="endOpen()" readonly  placeholder="">
						</strong>
						<div class="area-layer" id="end_area" style="display:none;left:350px !important; top :81px;">
							<div class="area-layer_b">
								<ul ID="arrArea">
									
								</ul>
							</div>
						</div>
					</div>

					<div class="search-item date-select" id="openCalendar">
						<span class="label">가는날</span>
						<strong class="date-span" id="dep_date_name" onClick="return generateCalendars('dep_date','CalendarLayer')">선택하세요</strong>
					</div>

					<div class="search-item date-select" id="openCalendarReturn">
						<span class="label">오는날</span>
						<strong class="date-span" id="arr_date_name" onClick="return generateCalendars('arr_date','CalendarLayer')">선택하세요</strong>
					</div>

					<div class="search-item pax-select" onClick="seatView()">
						<span class="label">인원</span>
						<strong id="seat-box">성인 1, 일반석</strong>
						<div style="position :relative;">
							<%- include ('mem.inc') %>
						</div>
					</div>
					
					<button class="search-btn" onClick="return searchAction()">🔍 검색</button>
	
					<!--//검색-->
					<div class='city_search_box' style="display:none" ID="CitySearchBox">
						<div class='pop_section'>
							<div class='pa815 title_bold'>도시정보</div>
							<img class='cal_close_img' src='/images/close_img.png'>
						</div>
						<div class='pa15 f13' ID="CitySearchBoxData" style="max-height:260px;overflow-y: auto;">
						</div>
					</div>
				</div>
				<!--//여행지역 선택-->
			</div>
			<div class='calendar-layer' ID='CalendarLayer'>
				<div class='layer-box'>
					<div class='layer-head'>
						<strong>날짜 선택</strong>
						<button class='close' onClick="calClose('CalendarLayer')">×</button>
					</div>
					<div class='calendar-layer-data' ID='CalendarLayerData'>
					</div>
				</div>
			</div>
	
			<input type="hidden" name="depCode"     value="GMP">
			<input type="hidden" name="arrCode"     value="">
		
			<input type="hidden" name="dep_date"    value="">
			<input type="hidden" name="arr_date"    value="">
		
			<input type="hidden" name="depTime1"    value="">
			<input type="hidden" name="depTime2"    value="">
			<input type="hidden" name="arrTime1"    value="">
			<input type="hidden" name="arrTime2"    value="">
			<input type="hidden" name="depAirCode"  value="">
			<input type="hidden" name="arrAirCode"  value="">
			<input type="hidden" name="depAirName"  value="">
			<input type="hidden" name="arrAirName"  value="">
			<input type="hidden" name="depAirClass" value="">
			<input type="hidden" name="arrAirClass" value="">
		
			<input type="hidden" name="depCodeShare" value="">
			<input type="hidden" name="arrCodeShare" value="">
		
			<input type="hidden" name="depUid"      value="">
			<input type="hidden" name="depIdx"      value="">
			<input type="hidden" name="depMinor"    value="">
			<input type="hidden" name="arrUid"      value="">
			<input type="hidden" name="arrIdx"      value="">
			<input type="hidden" name="arrMinor"    value="">
			<input type="hidden" name="depPrice"    value="">
			<input type="hidden" name="arrPrice"    value="">
			<input type="hidden" name="airType"     value="KE/OZ/7C/LJ/TW/BX/RS/RF/4H/4V/ZE/WE">
		
		
			<input type="hidden" name="adtMem"      value="1">
			<input type="hidden" name="chdMem"      value="0">
			<input type="hidden" name="infMem"      value="0">

			<input type="hidden" name="adtDepAmt"      value="0">
			<input type="hidden" name="chdDepAmt"      value="0">
			<input type="hidden" name="infDepAmt"      value="0">
			<input type="hidden" name="adtDepTax"      value="0">
			<input type="hidden" name="chdDepTax"      value="0">
			<input type="hidden" name="adtDepFuel"     value="0">
			<input type="hidden" name="chdDepFuel"     value="0">

			<input type="hidden" name="adtArrAmt"      value="0">
			<input type="hidden" name="chdArrAmt"      value="0">
			<input type="hidden" name="infArrAmt"      value="0">
			<input type="hidden" name="adtArrTax"      value="0">
			<input type="hidden" name="chdArrTax"      value="0">
			<input type="hidden" name="adtArrFuel"     value="0">
			<input type="hidden" name="chdArrFuel"     value="0">
		
			<input type="hidden" name="grade"       value="Y">
		
			<input type="hidden" name="DepCheck"    value="">
			<input type="hidden" name="ArrCheck"    value="">
		
			<input type="hidden" name="ticket_type" value="2">
	
			<div class="Search-Area">	
				<div class="search-wrap">
					<table class="form">
						<tbody>				
							<tr >
								<td class="th wh100">
									<div class="check-group check-group2 custom-checkbox" onClick="return airCheckAll()">
										<input class="custom-control-input" type="checkbox" checked name="air_ALL" id="air_ALL" value="Y"><label class="custom-control-label" for="air_ALL">전체</label>
									</div>
								</td>
								<td class="td" style="width:460px;" colspan="3">
									<%- include ('air.inc') %>
								</td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>

		<div style="display:flex;margin-top:10px;">
			<!--page_item_content-->
				<div class="page_item_content page_item_content2" id="SearchMain" style="display:none">	
	
					<!--list-wrap-->
					<div class="list-wrap" >
						<!--가는편-->
						<div class="list-wrap-50" style="margin-right:2%;">
							<p class="tit"><img src="/images/departure.png" class="icon_img">가는편</p>
							<p class="arr_txt" id="depCityName">출발지 -> 도착지</p>
							<div class="info_date">
								<span class="pre"><img src="/images/pre.png" class="info_control"></span>
								<span class="s1"><span id="depDateName"></span></span>
								<span class="next"><img src="/images/next.png" class="info_control"></span>
							</div>
							<div class="bar"></div>
							
							<!--list-->
							<div class="list air-list-form2">
								<div class="list-roll" ID="depAreaList">
									
								</div>
							</div>
							<!--//list-->
						</div>
						<!--//가는편-->
	
						<!--오는편-->
						<div class="list-wrap-50">
							<p class="tit"><img src="/images/arrive.png" class="icon_img">오는편</p>
							<p class="arr_txt" id="arrCityName">출발지 -> 도착지</p>
							<div class="info_date">
								<span class="pre"><img src="/images/pre.png" class="info_control"></span>
								<span class="s1"><span id="arrDateName"></span></span>
								<span class="next"><img src="/images/next.png" class="info_control"></span>
							</div>
							<div class="bar"></div>
							
							<!--list-->
							<div class="list air-list-form2">
								<div class="list-roll" ID="arrAreaList">
									
								</div>
							</div>
							<!--//list-->
						</div>
						<!--//오는편-->
	
					</div>
					<!--//list-wrap-->
				</div>
			<!--//page_item_content-->
				<div class="result-panel" style="margin-top: 10px; margin-left: 25px; margin-right: 25px; width: calc(100% - 50px);">
					<div class="page_item_content page_item_content2">
						<div id="SearchNotice" class="mat10">
							<div class="border regis-tle-box shadow-sm">
								<h5 class="regis-tle">
									<i class="fas fa-edit" style="color:#777;font-size:16px;"></i>&nbsp;&nbsp;항공사뉴스&nbsp;&nbsp;
								</h5>
							</div>
							<div ID=listFrameSub >
								
							</div>
						</div>
					</div>
				</div>
			<!--air-page-quickbar-->
			<!--//air-page-quickbar-->
	
	
			</div>
			

		</form>
	</div>
</div>


